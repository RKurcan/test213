    <script>
        var message = '@ViewBag.Message';
        var loginAs='@ViewBag.LoginAs';
        function RestrictSpace() {
            if (event.keyCode == 32) {
                event.returnValue = false;
                return false;
            }
        };
        $(document).ready(function () {
            if (message != "")
            {
                $("#LoginTab li").removeClass('active');
                switch (loginAs) {
                    case 'User':
                        $("#LoginTab li").eq(0).addClass('active').find('a').attr("aria-expanded","true").click();
                        break;
                    case 'Reseller':
                        $("#LoginTab li").eq(1).addClass('active').find('a').attr("aria-expanded", "true").click();
                        $('#myTabContent > div').removeClass('active in').eq(1).addClass('active in');
                        break;
                    default:
                }

                $('#msg').hide();
                $('#aLogin').click();
                toastr.error(message, "Login Failed");
            }
            setTimeout(function () {
                $('#msg').fadeOut('fast');
            }, 0);
        });
    </script>

</body>
</html>

