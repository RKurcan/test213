@if (Request.IsAjaxRequest())
{

    <script type="text/javascript">

        function setColumnVisible(reportTitle, colName, visible) {
            var reportFromStorage = {};
            if (localStorage.reportVisibility) {
                reportFromStorage = JSON.parse(localStorage.reportVisibility);
            }

            if (reportFromStorage[reportTitle]) {
                if (reportFromStorage[reportTitle][colName]) {
                    reportFromStorage[reportTitle][colName] = visible || false;
                }
            }
            else {
                reportFromStorage[reportTitle] = {
                };
            }
            reportFromStorage[reportTitle][colName] = visible;
            localStorage.reportVisibility = JSON.stringify(reportFromStorage);
        }

        function setColumFilter(item) {
            if (item.checked == true) {
                setColumnVisible(item.dataset.report, item.dataset.colname, true);
            }
            else {
                setColumnVisible(item.dataset.report, item.dataset.colname, false);
            }
            obj[item.dataset.report][item.dataset.colname](item.checked);
        }

        var obj = {};
        attendanceReportInit();
        function attendanceReportInit() {

            ko.cleanNode(document.getElementById("partial-render"));
            var config = new Riddha.config();
            var companyName = config.CurrentLanguage == "ne" && '@RTech.Demo.Utilities.RiddhaSession.CurrentUser.Branch.NameNp' != null ? '@RTech.Demo.Utilities.RiddhaSession.CurrentUser.Branch.NameNp' : '@RTech.Demo.Utilities.RiddhaSession.CurrentUser.Branch.Name';
            ko.cleanNode(document.getElementById("partial-render"));
            function reportObservableCreator(reportTitle, viewModel) {
                obj[reportTitle] = ko.observable({});
                for (var key in viewModel) {
                    obj[reportTitle][viewModel[key]] = ko.observable(obj.getColumnVisible(reportTitle, viewModel[key]));
                }
            }
            Riddha.global.permission.action('5001').done(function (data) {
                Riddha.global.permission.actions = data.Data;
                obj = new attendanceReportController('@DateTime.Now.ToString("yyyy/MM/dd")', companyName);
                obj['getColumnVisible'] = function getColumnVisible(reportTitle, colName) {
                    var reportFromStorage = {};
                    if (localStorage.reportVisibility) {
                        reportFromStorage = JSON.parse(localStorage.reportVisibility);
                    }
                    if (reportFromStorage[reportTitle]) {
                        if (reportFromStorage[reportTitle][colName] != undefined) {
                            if (reportFromStorage[reportTitle][colName]) {
                                return reportFromStorage[reportTitle][colName];
                            }
                        }
                        else {
                            setColumnVisible(reportTitle, colName, true);
                        }
                    }
                    else {
                        setColumnVisible(reportTitle, colName, true);
                        return true;
                    }
                };

                reportObservableCreator('EmployeeSummaryReportTable', [
                    'AbsentDays',
                    'DutyDays',
                    'HolidayDays',
                    'Kaj',
                    'LeaveDays',
                    'MiscDays',
                    'OTHours',
                    'OfficeVisit',
                    'PresentDays',
                    'PresentOnDayOff',
                    'PresentOnHoliday',
                    'Remarks',
                    'TotalDays',
                    'WeekendDays',
                    'WorkedHours',
                    'earlyin',
                    'earlyout',
                    'latein',
                    'lateout',
                    'remark']);

                reportObservableCreator('MonthlyAttendanceReport', [
                    'Mearlyin',
                    'Mearlyout',
                    'Mlatein',
                    'Mlateout',
                    'Mremark']);

                $('[data-report]').each(function () {
                    var item = $(this)[0];
                    item.checked = obj.getColumnVisible(item.dataset.report, item.dataset.colname);
                });
                ko.applyBindings(obj, document.getElementById("partial-render"));
            });
        }
    </script>
}

