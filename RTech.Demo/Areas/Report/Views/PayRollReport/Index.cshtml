@{
    ViewBag.Title = "Index";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Layout_Adminlte.cshtml";
}
<style>
    .panel-body {
        padding: 7.5px;
    }

    .list-group-item {
        position: relative;
        display: block;
        padding: 4px;
        margin-bottom: -1px;
        background-color: #fff;
        border: 1px solid #ddd;
    }

        .list-group-item:first-child {
            margin-top: 0;
            border-top: 0px;
            border-bottom-right-radius: 4px;
            /* border-bottom-left-radius: 4px; */
        }

    .modal.bootstrap-dialog.type-primary.fade.size-large.in > .modal-dialog {
        width: 90%;
        overflow-x: scroll;
    }

    .appbtn {
        /*display: inline-block;
        padding: 8px 8px;
        margin-bottom: 0;
        font-size: 13px;
        font-weight: normal;
        line-height: 1.428571429;*/
        border-radius: 3px;
        position: relative;
        padding: 15px 5px;
        margin: 0 0 10px 10px;
        min-width: 80px;
        height: 60px;
        text-align: center;
        color: #666;
        border: 1px solid #ddd;
        background-color: #f4f4f4;
        font-size: 12px;
    }

    input[type="checkbox"] {
        margin: 3px;
        padding: 0;
    }
</style>
<section class="content-header">
    <h1>
        @T("Payroll")
        <sup class="text-green">@T("Report")</sup>
    </h1>

    <ol class="breadcrumb">
        <li><a href="\"><i class="fa fa-dashboard"></i> @T("Home")</a></li>
        <li class="active">@T("PayrollReport")</li>
    </ol>
</section>
<section class="content">
    <!-- Report Modal -->


    <div class="modal" id="reportModel" style="">
        <div class="modal-dialog modal-lg ">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <header><h4 class="text-success" data-bind="text:$root.Report"></h4></header>
                </div>
                <div class="modal-body">
                    <div class="form-horizontal" data-bind="">
                        <div class="row" data-bind="visible:VisibleEndDate" style="display:none;">
                            <div class="form-group col-lg-4">
                                <label class="col-lg-4 control-label" for="OnDate">@T("Year")</label>
                                <div class="col-lg-5">
                                    <input type="text" class="form-control" data-bind="value:Year,mask:'number'" maxlength="4">
                                </div>
                            </div>
                            <div class="form-group col-lg-4">
                                <label class="col-lg-4 control-label">@T("Month")</label>
                                <div class="col-lg-8">
                                    <select class="form-control" data-bind="options:Months,optionsText:'Name',optionsValue:'Id',value:MonthId"></select>
                                </div>
                            </div>
                            <div class="form-group row col-md-4" data-bind="visible:PackageId()>0" style="display:none;">
                                <div class="form-group col-lg-5">
                                    <p><input type="radio" name="monthly-active-inactive" value="0" data-bind="checked:ActiveInactiveMode"> &nbsp; @T("Active")</p>
                                </div>
                                <div class="form-group col-lg-7">
                                    <p><input type="radio" name="monthly-active-inactive" value="1" data-bind="checked:ActiveInactiveMode">&nbsp; @T("IncludeInactive")</p>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-lg-4">
                                <label class="col-lg-4 control-label" for="OnDate">@T("OnDate")</label>
                                <div class="col-lg-8">
                                    <input type="text" class="form-control" data-nepdate="#nepaliDate" data-bind="mask:'date',value:OnDate,visible:DateVisibility('en')">
                                    <input class="form-control nepDate" data-format="yyyy/MM/dd" name="date" type="text" id="nepaliOnDate" data-bind="dateNp:{engDate:OnDate},visible:DateVisibility('ne')">
                                </div>
                            </div>
                            <div class="form-group row col-md-4" data-bind="visible:!VisibleEndDate() && PackageId()>0" style="display:none;">
                                <div class="form-group col-lg-5">
                                    <p><input type="radio" name="daily-active-inactive" value="0" data-bind="checked:ActiveInactiveMode"> &nbsp; @T("Active")</p>
                                </div>
                                <div class="form-group col-lg-7">
                                    <p><input type="radio" name="daily-active-inactive" value="1" data-bind="checked:ActiveInactiveMode">&nbsp; @T("Include Inactive")</p>
                                </div>
                            </div>
                            <div class="form-group col-lg-4" data-bind="visible:VisibleEndDate" style="display:none;">
                                <label class="col-lg-4 control-label" for="OnDate">@T("EndDate")</label>
                                <div class="col-lg-8">
                                    <input type="text" class="form-control" data-nepdate="#nepaliDate" data-bind="mask:'date',value:EndDate,visible:DateVisibility('en')">
                                    <input class="form-control nepDate" data-format="yyyy/MM/dd" name="date" type="text" id="nepaliEndDate" data-bind="dateNp:{engDate:EndDate},visible:DateVisibility('ne')">
                                </div>
                            </div>
                            <div class="form-group row col-md-4" data-bind="visible:ReportId()==16" style="display:none;">
                                <div class="form-group col-lg-6">
                                    <p><input type="radio" name="time-wise" value="1" data-bind="checked:Daywise"> &nbsp; @T("DayWise")</p>
                                </div>
                                <div class="form-group col-lg-6">
                                    <p><input type="radio" name="time-wise" value="0" data-bind="checked:Daywise">&nbsp; @T("HourWise")</p>
                                    <p><input type="checkbox" data-bind="checked:IsLeaveWiseSummary">&nbsp; @T("Include Leave")</p>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-3">
                                <div class="panel panel-success">
                                    <div class="box-header bg-green ">@T("Branch") <input class="pull-right" type="checkbox" data-bind="checked:CheckAllBranches" /></div>
                                    <div class="panel-body" style="height:250px; overflow-y:auto">
                                        <ul data-bind="foreach:Branches" class="list-group list list-group-unbordered">
                                            <li class="list-group-item">
                                                <span data-bind="text:Name"></span>   <a class="pull-right"><input type="checkbox" data-bind="checked:Checked,event:{change:$root.GetDepartments}" /></a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
