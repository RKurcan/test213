@{
    ViewBag.Title = "Index";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Layout_Adminlte.cshtml";
}
<section class="content-header">
    <h1>
        @T("EmployeeStatus")
        <sup class="text-green">@T("Verification")</sup>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> @T("Home")</a></li>
        <li class="active">@T("Verification")</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="modal" id="contractViewModel">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <header><h4 class="text-green text-center" data-bind="text:'Contract For: '+SelectedContract().EmployeeName()"></h4> </header>
                </div>
                <div class="modal-body">
                    <div class="form-horizontal" data-bind="with:Contract">
                        <div class="col-lg-1"></div>
                        <div class="col-lg-11">
                            <dl class="dl-horizontal">
                                <dt>@T("Code") :</dt>
                                <dd data-bind="text:Code"></dd>
                                <dt>@T("Employee") :</dt>
                                <dd data-bind="text:EmployeeName"></dd>
                                <dt>@T("EmploymentStatus") :</dt>
                                <dd data-bind="text:EmploymentStatusName"></dd>
                                <dt>@T("Period") :</dt>
                                <dd data-bind="text:Period"></dd>
                            </dl>
                        </div>
                    </div>
                    &nbsp;
                    <div class="form-group col-lg-12 text-center">
                        <input type="submit" data-bind="click:$root.Approve,actionVisibility:'7110'" name="Save" class="btn btn-success action" value=@T("Approve") />
                        <input type="button" data-bind="click:$root.CloseModal" value=@T("Back") class="btn btn-default" />
                    </div>
                    <div class="modal-footer">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal" id="resignationViewModel">
        <div class="modal-dialog ">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <header><h4 class="text-green text-center" data-bind="text:'Resignation For: '+SelectedResignation().EmployeeName()"></h4> </header>
                </div>
                <div class="modal-body">
                    <div class="form-horizontal" data-bind="with:Resignation">
                        <div class="col-lg-1"></div>
                        <div class="col-lg-11">
                            <dl class="dl-horizontal">
                                <dt>@T("Code") :</dt>
                                <dd data-bind="text:Code"></dd>
                                <dt>@T("Employee") :</dt>
                                <dd data-bind="text:EmployeeName"></dd>
                                <dt>@T("NoticeDate") :</dt>
                                <dd data-bind="text:SuitableDate(NoticeDate())"></dd>
                                <dt>@T("DesiredResignDate") :</dt>
                                <dd data-bind="text:SuitableDate(DesiredResignDate())"></dd>
                                <dt>@T("ForwardTo") :</dt>
                                <dd data-bind="text:ForwardToName"></dd>
                                <dt>@T("Reason") :</dt>
                                <dd data-bind="text:Reason"></dd>
                                <dt>@T("Details") :</dt>
                                <dd data-bind="text:Details"></dd>
                            </dl>
                        </div>
                    </div>
                    &nbsp;
                    <div class="form-group text-center">
                        <input type="submit" data-bind="click:$root.ResignationApprove,actionVisibility:'7116'" name="Save" class="btn btn-success action" value=@T("Approve") />
                        <input type="button" data-bind="click:$root.ResignationCloseModal" value=@T("Back") class="btn btn-default" />
                    </div>
                    <div class="modal-footer">
                    </div>
                </div>
            </div>
        </div>
    </div>
