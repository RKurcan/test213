    <div class="modal" id="qualificationCreationModel" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg ">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true" @*data-bind="click:CloseModal"*@>&times;</button>
                    <header><h4>@T("Employee") @T("Qualification")</h4></header>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="box box-default">
                                <div class="box-body table-responsive ">
                                    <table class="table table-condensed table-bordered table-striped no-padding table-hover">
                                        <thead class="success bg-green">
                                            <tr>
                                                <th class=" text-center"> @T("SerialNo")</th>

                                                <th class="col-lg-2 text-center"> @T("Code")</th>

                                                <th class="col-lg-3 text-center"> @T("Name")</th>

                                                <th class="col-lg-5 text-center"> @T("Description")</th>

                                                <th class="col-lg-2 text-center">@T("Action")</th>
                                            </tr>
                                        </thead>
                                        <tbody data-bind="foreach:EmployeeEducations">
                                            <tr>
                                                <td class="text-center" data-bind="serialNo:$index()+1"></td>
                                                <td class="text-center" data-bind="text:EducationCode"></td>
                                                <td class="text-center" data-bind="text:EducationName"></td>
                                                <td class="" data-bind="text:EducationDescription"></td>
                                                <td>
                                                    <button type="button" data-bind="click:$root.EmployeeEducationSelect" class="btn btn-xs btn-default" title=@T("Edit")><span class="glyphicon glyphicon-pencil text-success"></span> </button>
                                                    <button type="button" data-bind="click:$root.DeleteEmployeeEducation" class="btn btn-xs btn-default" title=@T("Remove")><span class="glyphicon glyphicon-trash text-danger"></span></button>
                                                </td>
                                            </tr>
                                        </tbody>
                                        <tfoot data-bind="with:Education">
                                            <tr>
                                                <td></td>
                                                <td>
                                                    <div class="">
                                                        <input type="text" class="form-control " data-bind="value:Code,event:{blur:$root.GetEducation}" placeholder=@T("Code") data-toggle="tooltip" title="Enter education code to search educations">
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="demo-section k-content">
                                                        <input data-bind="kendoAutocomplete:$root.EducationAutoCompleteOptions,value:Name" style="width: 100%;" placeholder="@T("Name")" data-toggle="tooltip" title="Type 3 underscore (___) to search all educations" />
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="">
                                                        <textarea class="form-control" data-bind="value:Description" placeholder=@T("Description") readonly></textarea>
                                                    </div>
                                                </td>
                                                <td>
                                                    <button type="button" data-bind="click:$root.EmployeeEducationCreateUpdate,template:{name:$root.EmployeeEducationModeOfButton()=='Create'?'mode-button-add-template':'mode-button-update-template'}" class="btn btn-success" title=@T("Create")></button>
                                                    <button type="button" data-bind="click:$root.ResetEmployeeEducation" class="btn btn-default" title=@T("Reset")><span class="glyphicon glyphicon-refresh"></span></button>
                                                </td>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <div class="col-lg-12 text-center">
                            <input type="button" id="close" data-bind="click:$root.TerminationCloseModal" value=@T("Back") class="btn btn-default" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal" id="empOtherInfoCreationModel" data-backdrop="static" data-keyboard="false">
