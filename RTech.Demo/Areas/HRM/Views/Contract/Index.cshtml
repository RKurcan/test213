@{
    ViewBag.Title = "Index";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Layout_Adminlte.cshtml";

}
<!-- toast container custom element, add this to your index page / default template -->

<section class="content-header">
    <h1>
        @T("Employee")
        <sup class="text-green">@T("Status")</sup>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> @T("Home")</a></li>
        <li class="active">@T("EmployeeStatus")</li>
    </ol>

</section>
<style>
    .form-control[disabled], .form-control[readonly], fieldset[disabled] .form-control {
        background-color: #ffffff;
        opacity: 1;
    }

    .btn-dropdown {
        height: 28px;
        border-radius: 3px;
    }
</style>
<!-- Main content -->
<section class="content">
    <!-- Section Modal -->
    <div class="modal" id="contractCreationModel">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <header><h4 class="text-green" data-bind="text:'@T("ContractFor"): '+SelectedEmployee().EmployeeName()"></h4> </header>
                </div>
                <div class="modal-body">
                    <div class="row" data-bind="with:Contract">
                        <div class="form-horizontal">
                            <div class="row">
                                <div class="col-lg-5 form-group">
                                    <label class="col-lg-4 control-label" for="Number">@T("Code")</label>
                                    <div class="col-lg-8">
                                        <input type="text" class="form-control" required data-bind="value:Code" id="form-group-input " placeholder=@T("Code")>
                                    </div>
                                </div>
                                <div class="col-lg-6 form-group">
                                    <div class="required">
                                        <label class="col-lg-5 control-label" for="Number">@T("EmploymentStatus")</label>
                                        <div class="col-lg-6">
                                            <select class="form-control" required data-bind="options:$root.EmploymentStatus,
                                                                                                optionsText:'Name',optionsValue:'Id',optionsCaption:Riddha.UI.getOptionCaption(),value:EmploymentStatusId"></select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-5 form-group">
                                    <label class="col-lg-4 control-label" for="Number">@T("BeganOn")</label>
                                    <div class="col-lg-6">
                                        <input type="text" class="form-control" data-nepdate="#nepaliDate" data-bind="mask:'date',value:BeganOn,visible:new Riddha.config().CurrentOperationDate=='en'">
                                        <input class="form-control nepDate" data-format="yyyy/MM/dd" name="date" type="text" id="contractnepaliBeganOn" data-bind="dateNp:{engDate:BeganOn},visible:new Riddha.config().CurrentOperationDate=='ne'">
                                    </div>
                                </div>
                                <div class="col-lg-6 form-group">
                                    <label class="col-lg-5 control-label" for="Number">@T("EndedOn")</label>
                                    <div class="col-lg-4">
                                        <input type="text" class="form-control" data-nepdate="#nepaliDate" data-bind="mask:'date',value:EndedOn,visible:new Riddha.config().CurrentOperationDate=='en'">
                                        <input class="form-control nepDate" data-format="yyyy/MM/dd" name="date" type="text" id="contractnepaliEndedOn" data-bind="dateNp:{engDate:EndedOn},visible:new Riddha.config().CurrentOperationDate=='ne'">
                                    </div>
                                </div>
                                <div class="col-lg-5 form-group">
                                    <label class="col-lg-4 control-label" for="Number">@T("File")</label>
                                    <div class="col-lg-4">
                                        <input type="file" data-bind="fileUpload:{value:FileUrl}" name="name" title="Please upload file having extensions PDF,Word & Excel only.">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    &nbsp;
                    <div class="row">
                        <div class="form-group text-center">
                            <input type="submit" data-bind="click:$root.CreateUpdate,createUpdateText:$root.ContractModeOfButton,actionVisibility:'7101'" name="Save" class="btn btn-success" value=@T("Create") />
                            <input type="button" data-bind="click:$root.Reset" name="Reset" value=@T("Reset") class="btn btn-default" />
                            <input type="button" id="close" data-bind="click:$root.CloseModal" value=@T("Back") class="btn btn-default" />
                        </div>
                    </div>
                    <div class="modal-footer">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="box box-success">
                                    <div class="box-header with-border">
                                        &nbsp;
                                    </div>
                                    <div class="box-body table-responsive ">
                                        <table class="table table-condensed table-bordered table-striped no-padding table-hover">
                                            <thead class="success bg-green">
                                                <tr>
                                                    <th class="col-lg-1 text-center"> @T("SerialNo")</th>

                                                    <th class="col-lg-1 text-center"> @T("Code")</th>

                                                    <th class="col-lg-2 text-center"> @T("Status")</th>

                                                    <th class="col-lg-2 text-center"> @T("BeganOn")</th>

                                                    <th class="col-lg-2 text-center"> @T("EndedOn")</th>

                                                    <th class="col-lg-2 text-center"> @T("File")</th>


                                                    <th class="col-lg-2 text-center">@T("Action")</th>
                                                </tr>
                                            </thead>
                                            <tbody data-bind="foreach:Contracts">
                                                <tr>
                                                    <td class="text-center" data-bind="serialNo:$index()+1"></td>
                                                    <td class="text-center" data-bind="text:Code"></td>
                                                    <td class="text-center" data-bind="text:EmploymentStatusName"></td>
                                                    <td class="text-center" data-bind="text:SuitableDate(BeganOn())"></td>
                                                    <td class="text-center" data-bind="text:SuitableDate(EndedOn())"></td>
                                                    <td class="text-center">
                                                        <a data-bind="attr:{href:FileUrl},visible:FileUrl()!=''" ><img style="width:40px;height:45px;" src="/Images/file-logo.png"></a>
                                                        <a data-bind="visible:FileUrl()==''" style="display: none;"><img style="width:40px;height:45px;" src="/Images/NoFIle1.png"></a>
                                                    </td>
                                                    <td class="text-center">
                                                        <button type="button" data-bind="click:$root.SelectContract,visible:$root.ActionVisibility('7102',ApprovedById)" class="btn btn-xs btn-default" title=@T("Edit")><span class="glyphicon glyphicon-pencil text-success"></span> </button>
                                                        <button type="button" data-bind="click:$root.Delete,visible:$root.ActionVisibility('7103',ApprovedById)" class="btn btn-xs btn-default" title=@T("Remove")><span class="glyphicon glyphicon-trash text-danger"></span></button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                            <tfoot data-bind="">
                                            </tfoot>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal" id="resignationCreationModel">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <header><h4 class="text-green" data-bind="text:'@T("ResignationFor"): '+SelectedEmployee().EmployeeName()"></h4> </header>
                </div>
                <div class="modal-body">
                    <fieldset data-bind="enable:Resignation().ApprovedById()==null">
                        <div class="row" data-bind="with:Resignation">
                            <div class="form-horizontal">
                                <div class="row">
                                    <div class="form-group col-lg-6">
                                        <div class="required">
                                            <label class="col-lg-5 control-label" for="Number">@T("Code")</label>
                                            <div class="col-lg-6">
                                                <input type="text" class="form-control" required data-bind="value:Code" id="form-group-input " placeholder=@T("Code")>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-6">
                                        <div class="required">
                                            <label class="col-lg-5 control-label" for="Number">@T("NoticeDate")</label>
                                            <div class="col-lg-4">
                                                <input type="text" class="form-control" data-nepdate="#nepaliDate" data-bind="mask:'date',value:NoticeDate,visible:new Riddha.config().CurrentOperationDate=='en'">
                                                <input class="form-control nepDate" data-format="yyyy/MM/dd" name="date" type="text" id="RegnepaliNoticeDate" data-bind="dateNp:{engDate:NoticeDate},visible:new Riddha.config().CurrentOperationDate=='ne'">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-6">
                                        <div class="required">
                                            <label class="col-lg-5 control-label" for="Number">@T("ForwardTo")</label>
                                            <div class="col-lg-6">
                                                <select class="form-control" data-bind="options:$root.ResignationEmployees,
                                    optionsText:'Name',optionsValue:'Id',value:ForwardToId,
                                    optionsCaption:Riddha.UI.getOptionCaption()"></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-6">
                                        <div class="required">
                                            <label class="col-lg-5 control-label" for="Number">@T("DesiredResignDate")</label>
                                            <div class="col-lg-4">
                                                <input type="text" class="form-control" data-nepdate="#nepaliDate" data-bind="mask:'date',value:DesiredResignDate,visible:new Riddha.config().CurrentOperationDate=='en'">
                                                <input class="form-control nepDate" data-format="yyyy/MM/dd" name="date" type="text" id="regnepaliDesiredResignDate" data-bind="dateNp:{engDate:DesiredResignDate},visible:new Riddha.config().CurrentOperationDate=='ne'">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-6">
                                        <div class="required">
                                            <label class="col-lg-5 control-label" for="Number">@T("Reason")</label>
                                            <div class="col-lg-6">
                                                <textarea class="form-control" maxlength="300" placeholder="@T("Reason")" data-bind="value:Reason"></textarea>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-6">
                                        <label class="col-lg-5 control-label" for="Number">@T("Details")</label>
                                        <div class="col-lg-6">
                                            <textarea class="form-control" maxlength="300" placeholder="@T("Details")" data-bind="value:Details"></textarea>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-6">
                                        <label class="col-lg-5 control-label" for="Number">@T("File")</label>
                                        <div class="col-lg-6">
                                            <a data-bind="attr:{href:FileUrl},visible:FileUrl()!=''"><img style="width:40px;height:45px;" src="/Images/file-logo.png"></a>
                                            <input type="file" data-bind="fileUpload:{value:FileUrl}" name="name" title="Please upload file having extensions PDF,Word & Excel only.">
                                            <a data-bind="visible:FileUrl()==''" style="display: none;"><img style="width:40px;height:45px;" src="/Images/NoFIle1.png"></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    &nbsp;
                    <div class="row">
                        <div class="form-group text-center" data-bind="visible:Resignation().ApprovedById()==null">
                            <input type="submit" data-bind="click:$root.CreateUpdateResignation,createUpdateText:$root.ResignationModeOfButton,actionVisibility:'7105'" name="Save" class="btn btn-success" value=@T("Create") />
                            <input type="button" data-bind="click:$root.ResetResignation" name="Reset" value=@T("Reset") class="btn btn-default" />
                            <input type="button" id="close" data-bind="click:$root.ResignationCloseModal" value=@T("Back") class="btn btn-default" />
                        </div>
                    </div>
                    <div class="modal-footer">

                    </div>
                    
                </div>
            </div>
        </div>
    </div>
    <div class="modal" id="terminationCreationModel">
